{
  "name": "learning-platform",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint:fs": "ls-lint",
    "lint": "next lint",
    "dev": "next dev",
    "build": "next build && pnpm run postbuild",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "seed": "cd supabase/seed && node index.mjs && cd ../../",
    "update-types": "npx supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" > src/lib/database.types.ts",
    "analyze": "ANALYZE=true next build && next bundle-analyzer",
    "preview": " pnpm run format-code && pnpm run build && next start",
    "storybook": "storybook dev -p 6006",
    "build:css": "npx tailwindcss -i src/global-style.css -o ./public/index.css",
    "build:storybook": "storybook build",
    "watch:css": "npx tailwindcss -i src/global-style.css -o index.css --watch",
    "watch:storybook": "pnpm run build:css && storybook dev -p 6006 -s ../public",
    "build-storybook": "storybook build",
    "prettier": "prettier -w ./src",
    "lint-fix": "pnpm eslint \"src/**/*.{ts,tsx}\" --resolve-plugins-relative-to . --max-warnings=0 --fix",
    "format-code": "pnpm run prettier && pnpm run lint-fix"
  },
  "dependencies": {
    "@anatoliygatt/use-prefers-reduced-motion": "^1.0.10",
    "@headlessui/react": "^1.7.14",
    "@hookform/resolvers": "^2.9.11",
    "@next-auth/firebase-adapter": "^2.0.1",
    "@next-auth/supabase-adapter": "^0.2.1",
    "@react-spring/web": "^9.7.2",
    "@sindresorhus/to-milliseconds": "^2.0.0",
    "@supabase/supabase-js": "^2.21.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@use-gesture/react": "^10.2.26",
    "@vercel/analytics": "^1.0.0",
    "app-root-path": "^3.1.0",
    "class-variance-authority": "^0.5.2",
    "enzyme": "^3.11.0",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "fuse.js": "^6.6.2",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.3.0",
    "next-auth": "^4.22.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.5",
    "nodemailer": "^6.9.1",
    "nprogress": "^0.2.0",
    "pretty-bytes": "^6.1.0",
    "pretty-ms": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-horizontal-scrolling-menu": "^3.2.5",
    "react-lite-youtube-embed": "^2.3.52",
    "react-loading-skeleton": "3.1.0",
    "react-markdown": "^7.1.2",
    "react-spinners": "^0.13.8",
    "react-swipeable": "^7.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-toastify": "^9.1.2",
    "react-use-downloader": "^1.2.4",
    "remark-gfm": "^3.0.1",
    "sharp": "^0.32.0",
    "simplebar-react": "^3.2.4",
    "supabase": "1.50.11",
    "use-deep-compare-effect": "^1.8.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.11.2",
    "@microsoft/tsdoc": "^0.14.2",
    "@next/bundle-analyzer": "^13.3.0",
    "@next/eslint-plugin-next": "^13.3.0",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/nextjs": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/testing-library": "0.0.14-next.2",
    "@svgr/webpack": "^7.0.0",
    "@types/mdx": "^2.0.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "autoprefixer": "^10.4.14",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.0.3",
    "encoding": "^0.1.13",
    "eslint": "^8.38.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^4.0.7",
    "postcss": "^8.4.22",
    "prettier": "^2.8.7",
    "storybook": "^7.0.6",
    "svgo": "^3.0.2",
    "svgo-loader": "^4.0.0",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.3.1",
    "tailwindcss-border-gradient-radius": "^3.0.1",
    "typescript": "^5.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,cjs}": [
      "pnpm prettier -w ./src",
      "pnpm eslint \"src/**/*.{ts,tsx}\" --resolve-plugins-relative-to . --max-warnings=0 --fix",
      "git add"
    ]
  },
  "main": "index.js"
}
